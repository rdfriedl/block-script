{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"includePattern": ".*\\.js$",
		"include": ["src/", "./README.md"],
		"exclude": ["src/lib"]
	},
	"opts": {
		"destination": "./docs/",
		"recurse": true,
		"access": "all",
		"template": "node_modules/ink-docstrap/template"
	},
	"templates": {
		"systemName": "Block - Script",
		"theme": "simplex",
		"linenums": true,
		"collapseSymbols": true,
		"inverseNav": true,
		"syntaxTheme": "light"
	}
}
