{
	"name": "block-script",
	"version": "1.0.0",
	"description": "a simple minecraft like ThreeJS example",
	"main": "server/main.js",
	"directories": {
		"src": "./src",
		"dist": "./dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rdfriedl/block-script.git"
	},
	"author": "rdfriedl",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/rdfriedl/block-script/issues"
	},
	"scripts": {
		"precommit": "lint-staged",
		"format": "prettier --write \"./**/*.{js,json,css,md}\"",
		"prebuild": "npm run clean",
		"build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.config.js --mode=production",
		"build-stats": "webpack --env.mode=production --env.status=true",
		"start": "webpack-dev-server --hot --env.mode=development",
		"pretest": "rimraf coverage",
		"test": "cross-env NODE_ENV=development TEST_COVERAGE=true karma start --single-run=true",
		"test-debug": "cross-env NODE_ENV=development karma start --browsers=Chrome --reporters mocha",
		"test-watch": "cross-env NODE_ENV=development karma start --reporters mocha",
		"clean": "rimraf dist",
		"predocs": "rimraf docs",
		"docs": "esdoc",
		"serve-docs": "npm run docs && serve docs --open",
		"upload-coverage": "codecov"
	},
	"lint-staged": {
		"*.{js,json,css,md}": [
			"prettier --write",
			"git add"
		]
	},
	"homepage": "https://github.com/rdfriedl/block-script#readme",
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.28",
		"@fortawesome/free-regular-svg-icons": "^5.13.0",
		"@fortawesome/free-solid-svg-icons": "^5.13.0",
		"@fortawesome/react-fontawesome": "^0.1.9",
		"file-saver": "^2.0.2",
		"keypress.js": "^2.1.5",
		"lodash": "^4.17.15",
		"prop-types": "^15.7.2",
		"qs": "^6.9.3",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-hot-loader": "^4.12.20",
		"react-imported-component": "^6.2.1",
		"react-router-dom": "^5.1.2",
		"stats.js": "^0.17.0",
		"styled-components": "^5.0.1",
		"three": "^0.115.0",
		"three.meshline": "^1.2.0",
		"undo-manager": "^1.0.5",
		"wolfy87-eventemitter": "^5.2.9"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-export-default-from": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.9.0",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-env": "^7.9.0",
		"@babel/preset-react": "^7.9.4",
		"autoprefixer": "^9.7.5",
		"babel-loader": "^8.1.0",
		"base64-font-loader": "^0.0.4",
		"chai": "^4.2.0",
		"codecov": "^3.6.5",
		"cross-env": "^7.0.2",
		"css-loader": "^3.4.2",
		"cssnano": "^4.1.10",
		"duplicate-package-checker-webpack-plugin": "^3.0.0",
		"esdoc": "^1.1.0",
		"esdoc-ecmascript-proposal-plugin": "^1.0.0",
		"esdoc-jsx-plugin": "^1.0.0",
		"esdoc-react-plugin": "^1.0.1",
		"esdoc-standard-plugin": "^1.0.0",
		"file-loader": "^6.0.0",
		"html-loader": "^1.0.0",
		"html-webpack-plugin": "^4.0.2",
		"husky": "^4.2.3",
		"ignore-loader": "^0.1.2",
		"img-loader": "^3.0.1",
		"imports-loader": "^0.8.0",
		"istanbul-instrumenter-loader": "^3.0.1",
		"karma": "^4.4.1",
		"karma-chrome-launcher": "^3.1.0",
		"karma-coverage-istanbul-reporter": "^2.1.1",
		"karma-firefox-launcher": "^1.3.0",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.5",
		"karma-sinon": "^1.0.5",
		"karma-sinon-chai": "^2.0.2",
		"karma-source-map-support": "^1.4.0",
		"karma-webpack": "^4.0.2",
		"lint-staged": "^10.0.9",
		"markdown-loader": "^5.1.0",
		"mini-css-extract-plugin": "^0.9.0",
		"mocha": "^7.1.1",
		"postcss-loader": "^3.0.0",
		"precss": "^4.0.0",
		"prettier": "^2.0.2",
		"raw-loader": "^4.0.0",
		"rimraf": "^3.0.2",
		"script-loader": "^0.7.2",
		"serve": "^11.3.0",
		"sinon": "^9.0.1",
		"sinon-chai": "^3.5.0",
		"style-loader": "^1.1.3",
		"text-transform-loader": "^2.0.0",
		"url-loader": "^4.0.0",
		"webpack": "^4.42.1",
		"webpack-bundle-analyzer": "^3.6.1",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3",
		"webpack-merge": "^4.2.2",
		"workbox-webpack-plugin": "^5.1.2"
	},
	"prettier": {
		"useTabs": true,
		"semi": true,
		"printWidth": 120
	}
}
