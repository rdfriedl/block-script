  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "build-dev": "cross-env NODE_ENV=dev webpack",
    "prebuild-docs": "npm run clean-docs",
    "build-docs": "jsdoc -c jsdoc.json",
    "start": "cross-env NODE_ENV=dev webpack-dev-server",
    "test": "karma start karma.conf.js",
    "clean": "rimraf dist",
    "clean-docs": "rimraf docs",
    "lint": "eslint src",
    "docs": "npm run build-docs && npm run server-docs",
    "server-docs": "live-server docs/ --open"
  },
  "homepage": "https://github.com/rdfriedl/block-script#readme",
