{
  "name": "block-script",
  "version": "1.0.0",
  "description": "a simple minecraft like ThreeJS example",
  "main": "server/main.js",
  "directories": {
    "src": "./src",
    "dist": "./dist"
  },
  "repository": {
    "type": "git",
